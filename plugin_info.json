{
  "name": "MindRoot Computer Use",
  "version": "1.0.0",
  "description": "Enables AI agents to control a virtual desktop environment using bytebot",
  "services": [
    "check_docker",
    "build_bytebot_image",
    "start_bytebot_container",
    "stop_bytebot_container",
    "get_bytebot_client"
  ],
  "commands": [
    "computer_check_docker",
    "computer_start",
    "computer_stop",
    "computer_screenshot",
    "computer_click",
    "computer_type",
    "computer_press_key",
    "computer_navigate",
    "computer_scroll",
    "computer_mouse_move",
    "computer_right_click",
    "computer_double_click",
    "computer_drag",
    "computer_get_cursor_position"
  ]
}
